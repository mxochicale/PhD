file = {:home/map479/mxochicale/phd/literature/chaos/embedology/Uzal, Grinblat, Verdes - 2011 - Optimal reconstruction of dynamical systems A noise amplification approach.pdf:pdf},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
mendeley-groups = {chaos\_embedology},
number = {1},
pmid = {21867289},
title = {{Optimal reconstruction of dynamical systems: A noise amplification approach}},
