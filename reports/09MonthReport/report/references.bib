@article{Frank2010,
abstract = {Activity recognition based on data from mobile wearable devices is becoming an important application area for machine learning. We propose a novel approach based on a combination of feature extraction using time-delay embedding and supervised learning. The computational requirements are considerably lower than existing approaches, so the processing can be done in real time on a low-powered portable device such as a mobile phone. We evaluate the performance of our algorithm on a large, noisy data set comprising over 50 hours of data from six different subjects, including activities such as running and walking up or down stairs. We also demonstrate the ability of the system to accurately classify an individual from a set of 25 people, based only on the characteristics of their walking gait. The system requires very little parameter tuning, and can be trained with small amounts of data.},
author = {Jordan Frank and Shie Mannor and Doina Precup},
file = {:home/map479/mxochicale/phd/literature/chaos/human\_motion/Frank, Mannor, Precup - 2010 - Activity and Gait Recognition with Time-Delay Embeddings Time-Delay Embeddings.pdf:pdf},
isbn = {9781577354666},
journal = {AAAI Conference on Artificial Intelligence},
keywords = {Special Track -- Physically Grounded AI},
mendeley-groups = {chaos\_human\_motion},
pages = {407--408},
title = {{Activity and Gait Recognition with Time-Delay Embeddings Time-Delay Embeddings}},
year = {2010}
}

@article{Caballero2014,
abstract = {In recent years, several works have explored variability using different approaches, trying to describe the variations in motor movement. Traditionally, movement variability was regarded as a system error due to noise of neuromuscular mechanisms, but alternative theories suggest that motor variability seems to reflect a functional behaviour improving motor control and enhancing learning. Controversial results have been reported about variability characteristics and its role in motor control and learning, and several works suggest that the main difficulty lies in how to measure this variability. In this work, we have outlined the most used non-linear tools to assess human variability, their applications, advantages and disadvantages. We have also suggested different methods about how to achieve a multidimensional approximation to motor variability. Finally, we have called attention to some methodological issues frequently reported as important aspects to take into account when measuring human movement variability.},
author = {Caballero, Carla and Barbado, David and Moreno, Francisco Javier},
file = {:home/map479/mxochicale/phd/literature/chaos/variability/Caballero, Barbado, Moreno - 2014 - Non-Linear Tools and Methodological Concerns Measuring Human Movement Variability an Overview.pdf:pdf},
journal = {European Journal of Human Movement},
keywords = {motor control,non-linear tools,time series RESUMEN,variability},
mendeley-groups = {chaos\_variability},
pages = {61--81},
title = {{Non-Linear Tools and Methodological Concerns Measuring Human Movement Variability: an Overview}},
volume = {32},
year = {2014}
}


