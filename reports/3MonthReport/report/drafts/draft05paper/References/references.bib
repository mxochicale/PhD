journal = {Medical \& biological engineering \& computing},
keywords = {Accidental Falls,Aged,Bayes Theorem,Female,Gait,Gait: physiology,Humans,Models, Biological,Probability,Prognosis,Risk Factors,Spatio-Temporal Analysis},
mendeley-groups = {HumanMotion},
month = feb,
number = {1-2},
pages = {29--37},
pmid = {23065654},
title = {{A dynamic Bayesian network for estimating the risk of falls from real gait data.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23065654},
volume = {51},
year = {2013}
}
