@article{Zhang2010,
abstract = {Reliable characterization of locomotor dynamics of human walking is vital to understanding the neuromuscular control of human locomotion and disease diagnosis. However, the inherent oscillation and ubiquity of noise in such non-strictly periodic signals pose great challenges to current methodologies. To this end, we exploit the state-of-the-art technology in pattern recognition and, specifically, dimensionality reduction techniques, and propose to reconstruct and characterize the dynamics accurately on the cycle scale of the signal. This is achieved by deriving a low-dimensional representation of the cycles through global optimization, which effectively preserves the topology of the cycles that are embedded in a high-dimensional Euclidian space. Our approach demonstrates a clear advantage in capturing the intrinsic dynamics and probing the subtle synchronization patterns from uni/bivariate oscillatory signals over traditional methods. Application to human gait data for healthy subjects and 
diabetics reveals a significant difference in the dynamics of ankle movements and ankle-knee coordination, but not in knee movements. These results indicate that the impaired sensory feedback from the feet due to diabetes does not influence the knee movement in general, and that normal human walking is not critically dependent on the feedback from the peripheral nervous system.},
author = {Zhang, Jie and Zhang, Kai and Feng, Jianfeng and Small, Michael},
doi = {10.1371/journal.pcbi.1001033},
file = {:home/mapx/xfiles/phd/literature/HumanMotion/Chaos\_IN\_HumanMotion/Zhang et al. - 2010 - Rhythmic dynamics and synchronization via dimensionality reduction application to human gait.pdf:pdf},
issn = {1553-7358},
journal = {PLoS computational biology},
keywords = {Ankle,Ankle: physiology,Biomechanical Phenomena,Computational Biology,Computational Biology: methods,Gait,Gait: physiology,Humans,Knee,Knee: physiology,Models, Biological,Walking,Walking: physiology},
mendeley-groups = {chaos\_IN\_HumanMotion},
month = jan,
number = {12},
pages = {e1001033},
pmid = {21187907},
title = {{Rhythmic dynamics and synchronization via dimensionality reduction: application to human gait.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3002994\&tool=pmcentrez\&rendertype=abstract},
volume = {6},
year = {2010}
}


